<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AlterableBottomSheetLayout">
        <!-- Foreground background, required -->
        <attr name="foreground" format="reference" />
        <!-- InnerLayout -->
        <attr name="head_layout" format="enum">
            <enum name="frame_layout" value="0" />
            <enum name="linear_layout" value="1" />
            <enum name="relative_layout" value="2" />
        </attr>
        <attr name="transparency_percent" format="float" />
        <attr name="margin_top" format="dimension" />
        <attr name="limited_area_span" format="dimension" />
        <attr name="hide_on_background_click" format="boolean" />
        <attr name="allow_limited_area_span" format="boolean" />
        <attr name="background_color" format="color" />
        <attr name="foreground_height" format="dimension">
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="foreground_color" format="color" />
        <attr name="isHidable" format="boolean" />
    </declare-styleable>
</resources>